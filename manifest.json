{
    "manifest_version": 3,
    "name": "AMS QR Code",
    "version": "1.0",
    "description": "A Chrome extension to display a QR code from the AMS API.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  